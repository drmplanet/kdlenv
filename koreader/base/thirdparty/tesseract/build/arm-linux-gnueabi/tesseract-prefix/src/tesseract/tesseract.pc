prefix=/usr/local
exec_prefix=${prefix}
bindir=${exec_prefix}/bin
datarootdir = ${prefix}/share
datadir=${datarootdir}
libdir=${exec_prefix}/lib
includedir=${prefix}/include/tesseract

Name: tesseract
Description: An OCR Engine that was developed at HP Labs between 1985 and 1995... and now at Google.
URL: https://code.google.com/p/tesseract-ocr
Version: 3.02.02
# Requires: lept  ## leptonica do not provide lept.pc
Libs: -L${libdir} -ltesseract /usr/lib/gcc-cross/arm-linux-gnueabi/4.7/libstdc++.a -Wl,-O1 -Wl,--as-needed -static-libstdc++ -L/root/koreader/base/thirdparty/zlib/build/arm-linux-gnueabi/zlib-prefix/src/zlib -L/root/koreader/base/thirdparty/libpng/build/arm-linux-gnueabi/libpng-prefix/src/libpng-build/lib -Wl,-rpath,\$$ORIGIN -Wl,-rpath-link,/root/koreader/base/thirdparty/zlib/build/arm-linux-gnueabi/zlib-prefix/src/zlib -Wl,-rpath-link,/root/koreader/base/thirdparty/libpng/build/arm-linux-gnueabi/libpng-prefix/src/libpng-build/lib -L/root/koreader/base/thirdparty/leptonica/build/arm-linux-gnueabi/leptonica-prefix/src/leptonica/src/.libs -llept -lpthread 
Cflags: -I${includedir} -O2 -ffast-math -pipe -fomit-frame-pointer  -march=armv7-a -mtune=cortex-a8 -mfpu=neon -mthumb -mfloat-abi=softfp -fno-finite-math-only -fno-stack-protector -U_FORTIFY_SOURCE -fPIC -O3  -I/root/koreader/base/thirdparty/leptonica/build/arm-linux-gnueabi/leptonica-prefix/src/leptonica/src/. -O2 -ffast-math -pipe -fomit-frame-pointer  -march=armv7-a -mtune=cortex-a8 -mfpu=neon -mthumb -mfloat-abi=softfp -fno-finite-math-only -fno-stack-protector -U_FORTIFY_SOURCE -fPIC -O3 -Iinclude_mod -DHAVE_CONFIG_H
